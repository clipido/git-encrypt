{
  "name": "git-encrypt",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "bin": {
    "git-encrypt": "./lib/index.js"
  },
  "repository": "https://github.com/stadro/git-encrypt.git",
  "author": "ingoandelhofs <ingoandelhofs@gmail.com>",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/index.ts",
    "build": "rm ./lib -rf && tsc -p . && copyfiles -u 1 './src/hooks/**' './lib/'",
    "fix-permissions": "sudo chmod +rx /usr/local/bin/git-encrypt",
    "install": "yarn i",
    "uninstall": "yarn u",
    "i": "yarn build && sudo npm i -g && yarn fix-permissions",
    "u": "sudo npm uninstall -g"
  },
  "devDependencies": {
    "@types/node": "^16.3.1",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "commander": "^8.0.0"
  }
}
